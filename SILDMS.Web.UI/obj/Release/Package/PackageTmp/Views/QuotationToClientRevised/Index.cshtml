<script type="text/javascript">

    app.controller('QuotationToClientRevisedCtrl', ['$scope', '$http', function ($scope, $http) {

        $scope.clientReqItemArray = [];
        $scope.clientReqTermArray = [];
        $scope.itemsByPage = 10;

        $http.get('/QuotationToClientRevised/GetServicesCategory')
            .success(function (response) {
                $scope.loading = false;
                $scope.serviceCategorys = response.result;
            }).error(function () { });

        $scope.QuotationToClientRevised = {
            QuotationToClientRevisedID: "",
            ClientID: "",
            ClientName: "",
            TermsID: "",
            FormName: "",
            RevisionNo: "",
            RevisionAmt: "",
            RevisionDate: "",
            Remarks: "",
            Status: ""
        }

        $scope.QuotationToClientRevisedItem = {
            QuotationToClientRevisedItemID: "",
            Description: "",
            DeliveryLocation: "",
            DeliveryDate: "",
            ServiceCategory: { ServiceCategoryID: "", ServiceCategoryName: "" },
            ServiceItem: { ServiceItemID: "", ServiceItemName: "" },
            DeliveryMode: "",
            ReqQnty: "",
            ReqUnit: ""
        }

        $('.DatePicker').datepicker({
            format: "dd/mm/yyyy",
            autoclose: true
            // Set minDate to 0 to disable selecting previous dates
        });

        $('.DatePicker')
            .on('changeDate', function (ev) {
                $('.datepicker').hide();
            });

        $('#DeliveryDate').datepicker({
            format: "dd/mm/yyyy",
            autoclose: false,
            minDate: new Date() // Set minDate to the current date to disable selecting previous dates
        }).on('changeDate', function (ev) {
            var EnteredDate = $('#DeliveryDate').val();
            var today = new Date();
            today.setHours(0, 0, 0, 0);

            var date = EnteredDate.substring(0, 2);
            var month = EnteredDate.substring(3, 5);
            var year = EnteredDate.substring(6, 10);
            var myDate = new Date(year, month - 1, date);
            myDate.setHours(0, 0, 0, 0);

            //if (myDate < today) {
            //    $('#DeliveryDate').val('');
            //    toastr.error("Purchase date should not be less than system date.");
            //}

            $('.datepicker').hide();
        });

        // ################# Detail Add Modal ####################
        $scope.AddWoItemModal = function () {
            if ($('#ClientName').val() == "") {
                toastr.error("Client Name Required");
                return;
            }

            if ($('#RevisionNo').val() == "") {
                toastr.error("Client WoNo.  Required");
                return;
            }

            if ($('#RevisionAmt').val() == "") {
                toastr.error("QuotationToClientRevised Amount Required.");
                return;
            }

            if ($('#RevisionDate').val() == "") {
                toastr.error("QuotationToClientRevised Date: Date Required");
                return;
            }
            $('#RequisitionItemModal').modal('show');
        };

        $scope.masterDataSetupGridData = [];  // base collection
        $scope.GridDisplayCollection = [].concat($scope.masterDataSetupGridData);

        $scope.AddWoItem = function () {

            if ($scope.QuotationToClientRevisedItem.ServiceCategory.ServiceCategoryID == "") {
                toastr.error("Service Category Required");
                return;
            }

            if ($scope.QuotationToClientRevisedItem.ServiceItem.ServiceItemID == "") {
                toastr.error("Service Item  Required");
                return;
            }

            if ($scope.QuotationToClientRevisedItem.DeliveryMode == "") {
                toastr.error("QuotationToClientRevised Amount Required.");
                return;
            }

            if ($scope.QuotationToClientRevisedItem.ReqUnit == "") {
                toastr.error("QuotationToClientRevised Date: Date Required");
                return;
            }

            if ($('#DeliveryLocation').val() == "") {
                toastr.error("Service Category Required");
                return;
            }

            if ($('#DeliveryDate').val() == "") {
                toastr.error("Service Item  Required");
                return;
            }

            if ($('#Description').val() == "") {
                toastr.error("QuotationToClientRevised Amount Required.");
                return;
            }

            if ($('#ReqQnty').val() == "") {
                toastr.error("QuotationToClientRevised Date: Date Required");
                return;
            }

            $scope.QuotationToClientRevisedItem.ServiceCategoryID = $scope.QuotationToClientRevisedItem.ServiceCategory.ServiceCategoryID;
            $scope.QuotationToClientRevisedItem.ServiceCategoryName = $scope.QuotationToClientRevisedItem.ServiceCategory.ServiceCategoryName;
            $scope.QuotationToClientRevisedItem.ServiceItemID = $scope.QuotationToClientRevisedItem.ServiceItem.ServiceItemID;
            $scope.QuotationToClientRevisedItem.ServiceItemName = $scope.QuotationToClientRevisedItem.ServiceItem.ServiceItemName;

            $scope.QuotationToClientRevisedItem.DeliveryMode = $scope.QuotationToClientRevisedItem.DeliveryMode;
            $scope.QuotationToClientRevisedItem.ReqUnit = $scope.QuotationToClientRevisedItem.ReqUnit;

            $scope.QuotationToClientRevisedItem.DeliveryLocation = $('#DeliveryLocation').val();
            $scope.QuotationToClientRevisedItem.DeliveryDate = $('#DeliveryDate').val();
            $scope.QuotationToClientRevisedItem.Description = $('#Description').val();
            $scope.QuotationToClientRevisedItem.ReqQnty = $('#ReqQnty').val();

            $scope.clientReqItemArray.push($scope.QuotationToClientRevisedItem);

            $scope.GridDisplayCollection = $scope.GridDisplayCollection.concat($scope.QuotationToClientRevisedItem);

            $scope.resetQuotationToClientRevisedItem();
            $("#btnAddWoItem").html('Add');
            $('#RequisitionItemModal').modal('hide');
        }

        $scope.resetQuotationToClientRevisedItem = function () {
            $('#DeliveryDate').val('');
            $scope.QuotationToClientRevisedItem = {
                QuotationToClientRevisedItemID: "",
                QuotationToClientRevisedID: "",
                ServiceCategory: { ServiceCategoryID: "", ServiceCategoryName: "" },
                ServiceItem: { ServiceItemID: "", ServiceItemName: "" },
                Description: "",
                DeliveryLocation: "",
                DeliveryDate: "",
                DeliveryMode: "",
                ReqQnty: "",
                ReqUnit: ""
            }
        }

        $scope.ShowClientList = function () {
            $http.get("/QuotationToClientRevised/GetClientInfoList")
                .success(function (response) {
                    console.log(response);
                    $scope.ClientList = response.ClientInfoList;
                    $scope.ClientDisplayedCollection = [].concat($scope.ClientList);
                    console.log($scope.ClientDisplayedCollection);
                    $scope.loading = false;
                })
                .error(function () { });

            $('#ClientInfoModal').modal('show');
        };

        var ServiceCategoryID = "";
        $scope.ServiceCategoryChange = function () {
            ServiceCategoryID = $scope.QuotationToClientRevisedItem.ServiceCategory.ServiceCategoryID;
            $http.post("/ServiceItemInfo/GetServiceItemInfoSearchList?ServiceItemCategoryID=" + ServiceCategoryID)
                .success(function (response) {
                    $scope.loading = false;
                    $scope.serviceItems = response.ServiceItemInfoSearchList;
                })
                .error(function () { });
        };

        $scope.clearQuotationToClientRevised = function () {
            $scope.QuotationToClientRevised = {
                QuotationToClientRevisedID: "",
                ClientID: "",
                ClientCode: "",
                ClientName: "",
                ContactPerson: "",
                ContactNumber: "",
                ClientTINNo: "",
                ClientBINNo: "",
                ClientAddress: "",
                RevisionNo: "",
                RevisionAmt: "",
                RevisionDate: "",
                Remarks: ""
            }

            $scope.QuotationToClientRevisedItem = {
                QuotationToClientRevisedItemID: "",
                QuotationToClientRevisedID: "",
                Description: "",
                DeliveryLocation: "",
                DeliveryDate: "",
                ServiceCategory: { ServiceCategoryID: "", ServiceCategoryName: "" },
                ServiceItem: { ServiceItemID: "", ServiceItemName: "" },
                DeliveryMode: "",
                ReqQnty: "",
                ReqUnit: ""
            }
            $scope.QuotationToClientRevisedTerm = {
                QuotationToClientRevisedTermsID: "",
                QuotationToClientRevisedID: "",
                TermsID: "",
                TermsCode: "",
                TermsName: ""
            }
            $("#DeliveryDate").val('');
            $scope.masterDataSetupGridData = [];  // base collection
            $scope.GridDisplayCollection = [].concat($scope.masterDataSetupGridData);

            $scope.ReqTermDataSetupGridData = [];  // base collection
            $scope.ReqTermGridDisplayCollection = [].concat($scope.ReqTermDataSetupGridData);
        }

        $scope.toggleReqItemEdit = function (tableRow, $i) {
            $('#RequisitionItemModal').modal('show');
            $scope.resetQuotationToClientRevisedItem();

            $scope.QuotationToClientRevisedItem.QuotationToClientRevisedItemID = tableRow.QuotationToClientRevisedItemID;

            $scope.QuotationToClientRevisedItem.ServiceCategory.ServiceCategoryID = tableRow.ServiceCategoryID;
            $scope.QuotationToClientRevisedItem.ServiceCategory.ServiceCategoryName = tableRow.ServiceCategoryName;
            $scope.QuotationToClientRevisedItem.ServiceItem.ServiceItemID = tableRow.ServiceItemID;
            $scope.QuotationToClientRevisedItem.ServiceItem.ServiceItemName = tableRow.ServiceItemName;

            $scope.QuotationToClientRevisedItem.DeliveryMode = tableRow.DeliveryMode;
            $scope.QuotationToClientRevisedItem.DeliveryLocation = tableRow.DeliveryLocation;
            $scope.QuotationToClientRevisedItem.DeliveryDate = tableRow.DeliveryDate;
            $scope.QuotationToClientRevisedItem.Description = tableRow.Description;

            $scope.QuotationToClientRevisedItem.ReqQnty = tableRow.ReqQnty;
            $scope.QuotationToClientRevisedItem.ReqUnit = tableRow.ReqUnit;

            $scope.GridDisplayCollection.splice($i, 1);
            $scope.clientReqItemArray.splice($i, 1);

            $http.post("/ServiceItemInfo/GetServiceItemInfoSearchList?ServiceItemCategoryID=" + tableRow.ServiceCategoryID)
                .success(function (response) {
                    $scope.loading = false;
                    $scope.serviceItems = response.ServiceItemInfoSearchList;
                })
                .error(function () { });

            $("#btnAddWoItem").html('Update');
        };

        $scope.toggleReqItemDelete = function (tableRow, $i) {
            if (!tableRow.QuotationToClientRevisedItemID) {
                $scope.resetQuotationToClientRevisedItem();
                $scope.GridDisplayCollection.splice($i, 1);
                $scope.clientReqItemArray.splice($i, 1);
                toastr.success('Data Deleted Successsfully');
            }
            else {
                $scope.resetQuotationToClientRevisedItem();
                $scope.QuotationToClientRevisedItem.QuotationToClientRevisedItemID = tableRow.QuotationToClientRevisedItemID;
                $('#deleteReqItemModal').modal('show');
            }
        };

        $scope.DeleteQuotationToClientRevisedItem = function () {
            $http.post('/QuotationToClientRevised/DeleteQuotationToClientRevisedItemAndTerm',
                { QuotationToClientRevisedItemID: $scope.QuotationToClientRevisedItem.QuotationToClientRevisedItemID, QuotationToClientRevisedTermID: $scope.QuotationToClientRevisedTerm.QuotationToClientRevisedTermID })
                .success(function (response) {
                    if (response.status == "S203") {
                        $scope.resetQuotationToClientRevisedItem();
                        $scope.GridDisplayCollection.splice($i, 1);
                        $scope.clientReqItemArray.splice($i, 1);
                        toastr.success('Data Deleted Successsfully');
                        return;
                    }
                });
        }

        ////////////////////////Select for Update from Search grid ///////////////////////

        $scope.toggleClientSelect = function (row) {
            $scope.QuotationToClientRevised.ClientID = row.ClientID;
            $scope.QuotationToClientRevised.ClientCode = row.ClientCode;
            $scope.QuotationToClientRevised.ClientName = row.ClientName;
            $scope.QuotationToClientRevised.ClientQutnAprvID = row.ClientQutnAprvID;

            //alert(row.ClientQutnAprvID);
            $http.post('/QuotationToClientRevised/GetQuotationToClientRevisedItemList', { ClientQutnAprvID: row.ClientQutnAprvID })
                .success(function (response) {
                    $scope.masterDataSetupGridData = response.QuotationToClientRevisedItemList;
                    $scope.GridDisplayCollection = [].concat($scope.masterDataSetupGridData);
                    $scope.loading = false;
                }).error(function () {
                    $scope.loading = false;
                });

            $http.post('/QuotationToClientRevised/GetQuotationToClientRevisedTermList', { ClientQutnAprvID: row.ClientQutnAprvID })
                .success(function (response) {
                    $scope.ReqTermDataSetupGridData = response.QuotationToClientRevisedTermList;
                    $scope.ReqTermGridDisplayCollection = [].concat($scope.ReqTermDataSetupGridData);
                    $scope.loading = false;
                }).error(function () {
                    $scope.loading = false;
                });

            $('#ClientInfoModal').modal('hide');
        };

        ///////////////////////Save/Update Master///////////////////////

        //////////////////Search Grid List///////////////////////////////

        $scope.SearchQuotationToClientRevisedModal = function () {
            $scope.GetQuotationToClientRevisedSearchList();
            $('#SearchQuotationToClientRevisedModal').modal('show');
        };

        $scope.GetQuotationToClientRevisedSearchList = function () {
            $http.post("/QuotationToClientRevised/GetQuotationToClientRevisedSearchList")
                .success(function (response) {
                    $scope.woInfoSearchList = response.woInfoSearchList;
                    $scope.clientReqDisplayedCollection = [].concat($scope.woInfoSearchList);
                })
                .error(function () { });
        }

        ////////////////////////Select for Update from Search grid ///////////////////////

        $scope.toggleQuotationToClientRevisedSelect = function (tableRow) {
            $scope.QuotationToClientRevised.QuotationToClientRevisedID = tableRow.QuotationToClientRevisedID;
            $scope.QuotationToClientRevised.ClientID = tableRow.ClientID;
            $scope.QuotationToClientRevised.ClientName = tableRow.ClientName;
            $scope.QuotationToClientRevised.RevisionNo = tableRow.RevisionNo;
            $scope.QuotationToClientRevised.RevisionDate = tableRow.RevisionDate;
            $scope.QuotationToClientRevised.RevisionAmt = tableRow.RevisionAmt;
            $scope.QuotationToClientRevised.Remarks = tableRow.Remarks;
            $scope.QuotationToClientRevised.Status = tableRow.Status;

            $http.post('/QuotationToClientRevised/GetQuotationToClientRevisedSearchItemList', { QuotationToClientRevisedID: tableRow.QuotationToClientRevisedID })
                .success(function (response) {
                    $scope.masterDataSetupGridData = response.QuotationToClientRevisedItemList;
                    $scope.GridDisplayCollection = [].concat($scope.masterDataSetupGridData);
                    $scope.loading = false;
                }).error(function () {
                    $scope.loading = false;
                });

            $http.post('/QuotationToClientRevised/GetQuotationToClientRevisedSearchTermsList', { QuotationToClientRevisedID: tableRow.QuotationToClientRevisedID })
                .success(function (response) {
                    $scope.ReqTermDataSetupGridData = response.QuotationToClientRevisedTermList;
                    $scope.ReqTermGridDisplayCollection = [].concat($scope.ReqTermDataSetupGridData);
                    $scope.loading = false;
                }).error(function () {
                    $scope.loading = false;
                });

            $('#SearchQuotationToClientRevisedModal').modal('hide');
        };

        ///////////////////////////////////////////////

        ////////////////////////Requsition Terms & Conditions ///////////////////////

        $scope.ShowTermsConditionsList = function () {
            $http.get("/QuotationToClientRevised/GetTermsConditionsList")
                .success(function (response) {
                    console.log(response);
                    $scope.TermsConditionsList = response.termsConditionsList;
                    $scope.TermsConditionsDisplayedCollection = [].concat($scope.TermsConditionsList);
                    console.log($scope.TermsConditionsDisplayedCollection);
                    $scope.loading = false;
                })
                .error(function () { });

            $('#QuotationToClientRevisedTermsConditionsModal').modal('show');
        };

        $scope.toggleTermsConditionsSelect = function (row) {
            $scope.QuotationToClientRevised.TermsID = row.TermsID;
            //$scope.QuotationToClientRevised.FormCode = row.FormCode;
            $scope.QuotationToClientRevised.FormName = row.FormName;

            $http.post('/QuotationToClientRevised/GetQuotationToClientRevisedTermAgainstFormList', { TermsID: row.TermsID })
                .success(function (response) {
                    $scope.ReqTermDataSetupGridData = response.QuotationToClientRevisedTermList;
                    $scope.ReqTermGridDisplayCollection = [].concat($scope.ReqTermDataSetupGridData);
                    $scope.loading = false;
                }).error(function () {
                    $scope.loading = false;
                });

            $('#QuotationToClientRevisedTermsConditionsModal').modal('hide');
        };

        $scope.QuotationToClientRevisedTerm = {
            QuotationToClientRevisedTermsID: "",
            QuotationToClientRevisedID: "",
            TermsID: "",
            TermsCode: "",
            TermsName: ""
        }

        $scope.AddReqTermModal = function () {
            if ($('#ClientName').val() == "") {
                toastr.error("Client Name Required");
                return;
            }

            if ($('#RevisionNo').val() == "") {
                toastr.error("Client WoNo.  Required");
                return;
            }

            if ($('#RevisionAmt').val() == "") {
                toastr.error("QuotationToClientRevised Amount Required.");
                return;
            }

            if ($('#RevisionDate').val() == "") {
                toastr.error("QuotationToClientRevised Date: Date Required");
                return;
            }
            $('#RequisitionTermModal').modal('show');
        };

        $scope.ReqTermDataSetupGridData = [];  // base collection
        $scope.ReqTermGridDisplayCollection = [].concat($scope.ReqTermDataSetupGridData);

        $scope.AddReqTerm = function () {

            if ($('#TermsCode').val() == "") {
                toastr.error("Terms Code Required");
                return;
            }

            if ($('#TermsName').val() == "") {
                toastr.error("Terms Name Required.");
                return;
            }

            $scope.QuotationToClientRevisedTerm.TermsCode = $('#TermsCode').val();
            $scope.QuotationToClientRevisedTerm.TermsName = $('#TermsName').val();

            $scope.clientReqTermArray.push($scope.QuotationToClientRevisedTerm);

            $scope.ReqTermGridDisplayCollection = $scope.ReqTermGridDisplayCollection.concat($scope.QuotationToClientRevisedTerm);

            $scope.resetQuotationToClientRevisedTerm();
            $("#btnAddReqTerm").html('Add');
            $('#RequisitionTermModal').modal('hide');
        }

        $scope.resetQuotationToClientRevisedTerm = function () {
            $scope.QuotationToClientRevisedTerm = {
                QuotationToClientRevisedTermsID: "",
                QuotationToClientRevisedID: "",
                TermsID: "",
                TermsCode: "",
                TermsName: ""
            }
        }

        $scope.toggleReqTermEdit = function (tableRow, $i) {
            $('#RequisitionTermModal').modal('show');
            $scope.resetQuotationToClientRevisedTerm();
            $scope.QuotationToClientRevisedTerm.QuotationToClientRevisedTermID = tableRow.QuotationToClientRevisedTermD;
            $scope.QuotationToClientRevisedTerm.QuotationToClientRevisedID = tableRow.QuotationToClientRevisedID;
            $scope.QuotationToClientRevisedTerm.TermsID = tableRow.TermsID;
            $scope.QuotationToClientRevisedTerm.TermsCode = tableRow.TermsCode;
            $scope.QuotationToClientRevisedTerm.TermsName = tableRow.TermsName;
            $scope.ReqTermGridDisplayCollection.splice($i, 1);
            $scope.clientReqTermArray.splice($i, 1);
            $("#btnAddReqTerm").html('Update');
        };

        $scope.toggleReqTermDelete = function (tableRow, $i) {
            if (!tableRow.QuotationToClientRevisedTermID) {
                $scope.resetQuotationToClientRevisedTerm();
                $scope.ReqTermGridDisplayCollection.splice($i, 1);
                $scope.clientReqTermArray.splice($i, 1);
                toastr.success('Data Deleted Successsfully');
            }
            else {
                $scope.resetQuotationToClientRevisedTerm();
                $scope.QuotationToClientRevisedTerm.QuotationToClientRevisedTermID = tableRow.QuotationToClientRevisedTermID;
                $('#deleteReqTermModal').modal('show');
            }
        };

        $scope.DeleteQuotationToClientRevisedTerm = function () {
            $http.post('/QuotationToClientRevised/DeleteQuotationToClientRevisedItemAndTerm',
                { QuotationToClientRevisedItemID: $scope.QuotationToClientRevisedItem.QuotationToClientRevisedItemID, QuotationToClientRevisedTermID: $scope.QuotationToClientRevisedTerm.QuotationToClientRevisedTermID })
                .success(function (response) {
                    if (response.status == "S203") {
                        $scope.resetQuotationToClientRevisedTerm();
                        $scope.ReqTermGridDisplayCollection.splice($i, 1);
                        $scope.clientReqTermArray.splice($i, 1);
                        toastr.success('Data Deleted Successsfully');
                        return;
                    }
                });
        }

        ///////////////////////////////////////////////

        $scope.Post = function () {

            if ($('#ClientName').val() == "") {
                toastr.error("Client Name Required");
                return;
            }

            if ($('#RevisionNo').val() == "") {
                toastr.error("Client WoNo.  Required");
                return;
            }

            if ($('#RevisionAmt').val() == "") {
                toastr.error("QuotationToClientRevised Amount Required.");
                return;
            }

            if ($('#RevisionDate').val() == "") {
                toastr.error("QuotationToClientRevised Date: Date Required");
                return;
            }

            $scope.QuotationToClientRevised.RevisionDate = $('#RevisionDate').val();
            $scope.QuotationToClientRevised.RevisionAmt = $('#RevisionAmt').val();

            $http.post('/QuotationToClientRevised/SaveQuotationToClientRevised',
                {
                    woInfo: $scope.QuotationToClientRevised,
                    woInfoItem: $scope.GridDisplayCollection,//$scope.clientReqItemArray,
                    woInfoTerm: $scope.ReqTermGridDisplayCollection
                })
                .success(function (response) {
                    if (response.status == "S201") {
                        toastr.success("Data Saved Successfully.");
                        $scope.clearQuotationToClientRevised();
                        $scope.loading = false;
                        return;
                    }
                    else if (response.status == "S202") {
                        toastr.success("Data Updated Successfully.");
                        $scope.clearQuotationToClientRevised();
                        $scope.loading = false;
                        return;
                    }
                    else if (response.status == "S203") {
                        toastr.success("Data Deleted Successfully.");
                        $scope.clearQuotationToClientRevised();
                        $scope.loading = false;
                        return;
                    }
                    else {
                        toastr.error("Data Save Failed.");
                        $scope.loading = false;
                        return;
                    }
                })
                .error(function () {
                    toastr.error("Data Save Failed.");
                    $scope.loading = false;
                    return;
                });
        }

        $scope.toggleRefreshTable = function () {
            window.location.reload();
        };
    }])
</script>
